[dmz_hosts]
dmz1 ansible_host=$DMZ_HOST ansible_user=root ansible_ssh_private_key_file=$DMZ_ACCESS_KEY

[dmz_hosts:vars]
cluster_name=$PROJECT_NAME
cluster_subnet=$CLUSTER_SUBNET
cluster_interface=eth1
talosconfig_src=$TALOSCONFIG
cluster_endpoint=$CLUSTER_IP
linode_credentials=$CLOUD_PROVIDER_PAT
cloud_provider_region=$CLOUD_PROVIDER_REGION
age_private_key=$AGE_PRIVATE_KEY
nginx_conf=$NGINX_CONF
turnserver_conf=$TURNSERVER_CONF
kernel_tuning_conf=$KERNEL_TUNING_CONF
